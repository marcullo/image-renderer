  "version": "0.2.0",
  "configurations": [
    {
      "name": "commander",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/_build/commander",
      "cwd": "${workspaceRoot}/_build",
      "stopAtEntry": true,
      "externalConsole": false,
    },
    {
      "name": "renderer",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/_build/renderer",
      "cwd": "${workspaceRoot}/_build",
      "stopAtEntry": true,
      "externalConsole": false,
    },
  ]
}
